@model IEnumerable<ProjectListViewModel>
<table>
    <tr>
        <th>Title</th>
    </tr>
    @foreach (var item in Model)
    {
        <tr>
            <td>@item.Title</td>
            <td><form asp-controller="Project" asp-action="Detail" asp-route-id="@item.Id"><button class="btn btn-primary" type="submit">Details</button></form></td>
        </tr>
    }
</table>
@model IEnumerable<ProjectListViewModel>
<table>
    <tr>
        <th>Title</th>
    </tr>
    @foreach (var item in Model)
    {
        <tr>
            <td>@item.Title</td>
            <td><a asp-controller="Project" asp-action="Detail" asp-route-id="@item.Id">Details</a></td>
        </tr>
    }
</table>